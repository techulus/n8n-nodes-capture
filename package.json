{
	"name": "@techulus/n8n-nodes-capture",
	"version": "0.1.0",
	"description": "n8n node for Capture Page API - capture website screenshots, generate PDFs, extract content and metadata",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"capture",
		"screenshot",
		"pdf",
		"website"
	],
	"license": "MIT",
	"homepage": "https://capture.page",
	"author": {
		"name": "Arjun Komath",
		"email": "arjunkomath@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/techulus/n8n-nodes-capture.git"
	},
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=7.18"
	},
	"packageManager": "pnpm@7.18.0",
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -s"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/CaptureApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Capture/Capture.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^24.3.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.42.0",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^2.7.1",
		"typescript": "^5"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
